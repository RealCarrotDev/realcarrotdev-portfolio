---
import BaseLayout from "../../layouts/BaseLayout.astro";
import HorizontalCard from "../../components/HorizontalCard.astro";
---

<BaseLayout title="Doctor's Commissions" sideBarActiveItemID="projects">
  
  <div class="mb-6">
    <a href="/projects" class="btn btn-outline btn-sm gap-2">
      <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><line x1="19" y1="12" x2="5" y2="12"></line><polyline points="12 19 5 12 12 5"></polyline></svg>
      Back to Portfolio
    </a>
  </div>

  <div class="text-5xl font-bold mb-2">Commissions for Doctor</div>
  <div class="text-xl py-2 mb-10 text-gray-400">
    Extensive collection of custom animations including the Pikachu Series, Pokemon, Digimon, and Yugioh assets.
  </div>

  <details class="collapse collapse-arrow bg-base-300 mb-6">
    <summary class="collapse-title text-2xl font-bold">Fat Pikachu Variations</summary>
    <div class="collapse-content">
      <div class="pt-4">
        
        {
          [
            { name: "1. Original Fat Pikachu", folder: "pikachu_fat" },
            { name: "2. Balloon Pikachu", folder: "pikachu_balloon" },
            { name: "3. Luffy Pikachu", folder: "pikachu_luffy" },
            { name: "4. Zuko Pikachu", folder: "pikachu_zuko" },
            { name: "5. Sailor Moon Pikachu", folder: "pikachu_sailormoon" },
            { name: "6. Mafia Pikachu", folder: "pikachu_mafia" },
            { name: "7. Miner Pikachu", folder: "pikachu_miner" },
            { name: "8. Mob Psycho Pikachu", folder: "pikachu_mobpsycho" },
            { name: "9. Snow Pikachu", folder: "pikachu_snow" },
            { name: "10. Surf Pikachu", folder: "pikachu_surf" },
            { name: "11. Dragonite Costume", folder: "pikachu_dragonite" },
            { name: "12. Bug Costume", folder: "pikachu_bug" },
            { name: "13. Metal Costume", folder: "pikachu_metal" },
            { name: "14. Poison Costume", folder: "pikachu_poison" },
          ].map((char) => (
            <details class="collapse collapse-plus bg-base-100 mb-2 border border-base-content/10">
              <summary class="collapse-title font-bold">{char.name}</summary>
              <div class="collapse-content">
                <div class="grid grid-cols-2 md:grid-cols-4 gap-4 pt-4 pb-4">
                  <div class="card bg-base-100 shadow-md border border-base-200"><figure class="h-32 flex items-center justify-center bg-base-200/50 p-2"><img src={`/projects/doctor/${char.folder}/idle.gif`} class="max-h-full max-w-full object-contain" /></figure><div class="text-center text-xs font-bold py-1 bg-base-300">Idle</div></div>
                  <div class="card bg-base-100 shadow-md border border-base-200"><figure class="h-32 flex items-center justify-center bg-base-200/50 p-2"><img src={`/projects/doctor/${char.folder}/battle.gif`} class="max-h-full max-w-full object-contain" /></figure><div class="text-center text-xs font-bold py-1 bg-base-300">Battle</div></div>
                  <div class="card bg-base-100 shadow-md border border-base-200"><figure class="h-32 flex items-center justify-center bg-base-200/50 p-2"><img src={`/projects/doctor/${char.folder}/faint.gif`} class="max-h-full max-w-full object-contain" /></figure><div class="text-center text-xs font-bold py-1 bg-base-300">Faint</div></div>
                  <div class="card bg-base-100 shadow-md border border-base-200"><figure class="h-32 flex items-center justify-center bg-base-200/50 p-2"><img src={`/projects/doctor/${char.folder}/physical.gif`} class="max-h-full max-w-full object-contain" /></figure><div class="text-center text-xs font-bold py-1 bg-base-300">Physical</div></div>
                  <div class="card bg-base-100 shadow-md border border-base-200"><figure class="h-32 flex items-center justify-center bg-base-200/50 p-2"><img src={`/projects/doctor/${char.folder}/special.gif`} class="max-h-full max-w-full object-contain" /></figure><div class="text-center text-xs font-bold py-1 bg-base-300">Special</div></div>
                  <div class="card bg-base-100 shadow-md border border-base-200"><figure class="h-32 flex items-center justify-center bg-base-200/50 p-2"><img src={`/projects/doctor/${char.folder}/quirk1.gif`} class="max-h-full max-w-full object-contain" /></figure><div class="text-center text-xs font-bold py-1 bg-base-300">Quirk 1</div></div>
                  <div class="card bg-base-100 shadow-md border border-base-200"><figure class="h-32 flex items-center justify-center bg-base-200/50 p-2"><img src={`/projects/doctor/${char.folder}/quirk2.gif`} class="max-h-full max-w-full object-contain" /></figure><div class="text-center text-xs font-bold py-1 bg-base-300">Quirk 2</div></div>
                  <div class="card bg-base-100 shadow-md border border-base-200"><figure class="h-32 flex items-center justify-center bg-base-200/50 p-2"><img src={`/projects/doctor/${char.folder}/quirk3.gif`} class="max-h-full max-w-full object-contain" /></figure><div class="text-center text-xs font-bold py-1 bg-base-300">Quirk 3</div></div>
                  <div class="card bg-base-100 shadow-md border border-base-200"><figure class="h-32 flex items-center justify-center bg-base-200/50 p-2"><img src={`/projects/doctor/${char.folder}/walk.gif`} class="max-h-full max-w-full object-contain" /></figure><div class="text-center text-xs font-bold py-1 bg-base-300">Walk</div></div>
                  <div class="card bg-base-100 shadow-md border border-base-200"><figure class="h-32 flex items-center justify-center bg-base-200/50 p-2"><img src={`/projects/doctor/${char.folder}/cry.gif`} class="max-h-full max-w-full object-contain" /></figure><div class="text-center text-xs font-bold py-1 bg-base-300">Cry</div></div>
                  <div class="card bg-base-100 shadow-md border border-base-200"><figure class="h-32 flex items-center justify-center bg-base-200/50 p-2"><img src={`/projects/doctor/${char.folder}/sleep.gif`} class="max-h-full max-w-full object-contain" /></figure><div class="text-center text-xs font-bold py-1 bg-base-300">Sleep</div></div>
                </div>
              </div>
            </details>
          ))
        }
        </div>
    </div>
  </details>


  <details class="collapse collapse-arrow bg-base-300 mb-6">
    <summary class="collapse-title text-2xl font-bold">Other Pokemon</summary>
    <div class="collapse-content">
      <div class="pt-4">
        
        <details class="collapse collapse-plus bg-base-100 mb-2 border border-base-content/10">
          <summary class="collapse-title font-bold">Mechamew</summary>
          <div class="collapse-content">
            <div class="grid grid-cols-2 md:grid-cols-4 gap-4 pt-4 pb-4">
              <div class="card bg-base-100 shadow-md border border-base-200"><figure class="h-32 flex items-center justify-center bg-base-200/50 p-2"><img src="/projects/doctor/mechamew/idle.gif" class="max-h-full max-w-full object-contain" /></figure><div class="text-center text-xs font-bold py-1 bg-base-300">Idle</div></div>
              <div class="card bg-base-100 shadow-md border border-base-200"><figure class="h-32 flex items-center justify-center bg-base-200/50 p-2"><img src="/projects/doctor/mechamew/battle.gif" class="max-h-full max-w-full object-contain" /></figure><div class="text-center text-xs font-bold py-1 bg-base-300">Battle</div></div>
              <div class="card bg-base-100 shadow-md border border-base-200"><figure class="h-32 flex items-center justify-center bg-base-200/50 p-2"><img src="/projects/doctor/mechamew/faint.gif" class="max-h-full max-w-full object-contain" /></figure><div class="text-center text-xs font-bold py-1 bg-base-300">Faint</div></div>
